
package percabangan1mj;

public class Percabangan1MJ {

    public static void main(String[] args) {
        int nilai = 90;
        if (nilai >= 96){
            System.out.println("Lulus");
        }else if (nilai <= 70){
            System.out.println("Mengulang");
        }else{
            System.out.println("Tidak Lulus");
        }
    
    }
    
}
